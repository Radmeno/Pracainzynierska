<Window x:Class="Praca_inzynierska_program_magazynowy.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praca_inzynierska_program_magazynowy"
        mc:Ignorable="d"
        Title="Login" Height="400" Width="400">
    <Grid x:Name="Loginn">
        <Label x:Name="___Brak_nazwy_" Content="Login" HorizontalAlignment="Left" Margin="51,101,0,0" VerticalAlignment="Top" Width="88"/>
        <TextBox x:Name="txtUserName" HorizontalAlignment="Left" Height="23" Margin="204,101,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Hasło" HorizontalAlignment="Left" Margin="51,176,0,0" VerticalAlignment="Top" Width="88"/>
        <PasswordBox x:Name="txtPassword" HorizontalAlignment="Left" Margin="204,184,0,0" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="buttonLogin" Content="Logowanie" HorizontalAlignment="Left" Margin="151,275,0,0" VerticalAlignment="Top" Width="75" Click="buttonLogin_Click" RenderTransformOrigin="1.24,0.131"/>

    </Grid>
</Window>
